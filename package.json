    "gatsby-plugin-graphql-codegen": "^3.0.0",
    "gatsby-plugin-graphql-loader": "^1.0.2",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.1",
    "gatsby-plugin-typescript": "^3.14.0",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-source-instagram": "^0.9.0",
    "gatsby-transformer-json": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-base64": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "json-server": "^0.14.2",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "pinch-zoom-js": "^2.3.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^8.0.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-localstorage": "^0.4.1",
    "typesafe-actions": "^5.1.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/b_": "^1.3.1",
