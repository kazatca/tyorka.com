    "@types/uuid": "^8.0.0",
    "comment-json": "^3.0.2",
    "dominant-color": "0.0.1",
    "flat": "^5.0.0",
    "gatsby-plugin-sass": "^4.14.0",
    "glob": "^7.1.6",
