{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "concurrently --kill-others --raw \"npm:server\" \"npm:develop\"",
    "format": "prettier --write \"src/**/*.js\"",
    "server": "nodemon --watch './src/server/*' -e ts --exec ts-node ./src/server/index.ts",
    "locales": "ts-node ./src/lib/locales",
    "update:products": "ts-node ./src/server/products.ts"
  },
  "dependencies": {
    "b_": "^1.3.4",
    "concurrently": "^5.2.0",
    "firebase": "^7.14.2",
    "gatsby": "^3.14.2",
